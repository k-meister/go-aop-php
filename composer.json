{
    "name": "k-meister/go-aop-php",
    "description": "Forked by k-meister: Library for aspect-oriented programming in PHP.",

    "type": "library",
    "keywords": ["library", "aop", "php", "aspect"],
    "homepage": "http://go.aopphp.com/",
    "license": "MIT",

    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/k-meister/PHP-Token-Reflection.git"
        }
    ],

    "require": {
        "php": ">=5.4.0",
        "jakubledl/dissect": "~1.0",
        "doctrine/annotations": "~1.0",
        "k-meister/php-token-reflection": "dev-develop"
    },

    "require-dev": {
        "symfony/console": "~2.1"
    },

    "suggest": {
        "symfony/console": "Enables the usage of the command-line tool."
    },

    "bin": ["bin/warmup"],
    "authors": [
        {
            "name": "Lisachenko Alexander",
            "homepage": "https://github.com/lisachenko"
        }
    ],

    "autoload": {
        "psr-4": {"Go\\": "src/"}
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
